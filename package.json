{
  "name": "@echo-karriere/clubhouse-lint",
  "version": "0.3.0",
  "license": "MIT",
  "repository": "echo-karriere/clubhouse-lint",
  "author": {
    "name": "Sondre Nilsen",
    "email": "sondre.nilsen@echokarriere.no",
    "url": "https://www.eons.io/"
  },
  "main": "dist/index",
  "bin": {
    "clubhouse-lint": "./bin"
  },
  "files": [
    "README.md",
    "LICENSE",
    "bin",
    "src/index.ts",
    "dist/index.js"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint:ts": "eslint src/**/*.ts",
    "lint:style": "prettier --check src/**/*.ts",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {},
  "devDependencies": {
    "@sondr3/eslint-config": "0.6.0",
    "@sondr3/prettier": "0.5.1",
    "@sondr3/tsconfig": "0.7.0",
    "@types/jest": "26.0.23",
    "@types/node": "15.12.4",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "husky": "7.0.0",
    "jest": "27.0.6",
    "lint-staged": "11.0.0",
    "pinst": "2.1.6",
    "prettier": "2.3.2",
    "prettier-eslint": "12.0.0",
    "ts-jest": "27.0.3",
    "typescript": "4.3.5"
  },
  "prettier": "@sondr3/prettier",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
