{
  "name": "@echo-karriere/clubhouse-lint",
  "version": "0.3.0",
  "license": "MIT",
  "repository": "echo-karriere/clubhouse-lint",
  "author": {
    "name": "Sondre Nilsen",
    "email": "sondre.nilsen@echokarriere.no",
    "url": "https://www.eons.io/"
  },
  "main": "dist/index",
  "bin": {
    "clubhouse-lint": "./bin"
  },
  "files": [
    "README.md",
    "LICENSE",
    "bin",
    "src/index.ts",
    "dist/index.js"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint:ts": "eslint src/**/*.ts",
    "lint:style": "prettier --check src/**/*.ts",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {},
  "devDependencies": {
    "@sondr3/eslint-config-typescript": "0.11.10",
    "@sondr3/prettier": "0.5.1",
    "@sondr3/tsconfig": "0.7.0",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.1",
    "eslint": "7.27.0",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-node": "11.1.0",
    "husky": "6.0.0",
    "jest": "27.0.2",
    "lint-staged": "11.0.0",
    "pinst": "2.1.6",
    "prettier": "2.3.0",
    "ts-jest": "27.0.1",
    "typescript": "4.2.4"
  },
  "prettier": "@sondr3/prettier",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
