{
  "name": "@echo-karriere/clubhouse-lint",
  "version": "0.2.1",
  "license": "MIT",
  "repository": "echo-karriere/clubhouse-lint",
  "author": {
    "name": "Sondre Nilsen",
    "email": "sondre.nilsen@echokarriere.no",
    "url": "https://www.eons.io/"
  },
  "main": "dist/index",
  "bin": {
    "clubhouse-lint": "./bin"
  },
  "files": [
    "README.md",
    "LICENSE",
    "bin",
    "src/index.ts",
    "dist/index.js"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint:ts": "eslint src/**/*.ts",
    "lint:style": "prettier --check src/**/*.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@sondr3/eslint-config-typescript": "0.11.10",
    "@sondr3/prettier": "0.5.0",
    "@sondr3/tsconfig": "0.7.0",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.20",
    "eslint": "7.19.0",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-node": "11.1.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  },
  "prettier": "@sondr3/prettier",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "./bin"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
