{
  "name": "@echo-karriere/clubhouse-lint",
  "version": "0.3.0",
  "license": "MIT",
  "repository": "echo-karriere/clubhouse-lint",
  "author": {
    "name": "Sondre Nilsen",
    "email": "sondre.nilsen@echokarriere.no",
    "url": "https://www.eons.io/"
  },
  "main": "dist/index",
  "bin": {
    "clubhouse-lint": "./bin"
  },
  "files": [
    "README.md",
    "LICENSE",
    "bin",
    "src/index.ts",
    "dist/index.js"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint:ts": "eslint src/**/*.ts",
    "lint:style": "prettier --check src/**/*.ts",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {},
  "devDependencies": {
    "@sondr3/eslint-config": "0.7.1",
    "@sondr3/prettier": "0.6.0",
    "@sondr3/tsconfig": "0.7.0",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.12",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "26.9.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.5.0",
    "pinst": "3.0.0",
    "prettier": "2.8.4",
    "prettier-eslint": "13.0.0",
    "ts-jest": "27.1.5",
    "typescript": "4.9.5"
  },
  "prettier": "@sondr3/prettier",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
